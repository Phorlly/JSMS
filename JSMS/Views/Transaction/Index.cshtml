<div class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1 class="m-0 text-uppercase">@JSMS.Resources.Language.TransactionManagement</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="@Url.Action("Index","Home")">@JSMS.Resources.Language.Home</a>
                    </li>
                    <li class="breadcrumb-item">@JSMS.Resources.Language.Transaction</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</div>
<div id="loading-gif"></div>
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header  pl-4">
                <div class="row">
                    <div class="col-md-6">
                        <ul class="nav nav-pills ml-auto p-2">
                            <li class="nav-item">
                                <a class="nav-link active" id="tab-income" href="#table-income" data-toggle="tab">
                                    <i class="fas fa-arrow-alt-circle-right"></i>
                                    <span>@JSMS.Resources.Language.Income</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="tab-expense" href="#table-expense" data-toggle="tab">
                                    <i class="fas fa-arrow-alt-circle-left"></i>
                                    <span>@JSMS.Resources.Language.Expense</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="tab-transaction" href="#table-transaction" data-toggle="tab">
                                    <i class="fas fa-file-signature"></i>
                                    <span>@JSMS.Resources.Language.Transaction</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6 mt-2">
                        <div class="float-sm-right mr-5">
                            <button type="button" id="add-income" class="btn btn-primary btn-sm">
                                <i class="fas fa-arrow-circle-down"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.Income</span>
                            </button>
                            <button type="button" id="add-expense" class="btn btn-secondary btn-sm">
                                <i class="fas fa-arrow-alt-circle-up"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.Expense</span>
                            </button>
                            <button type="button" id="refresh" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.ShowData</span>
                            </button>
                        </div>
                    </div>
                </div>

            </div><!-- /.card-header -->

            <div class="card-body">
                <div class="tab-content">
                    <div class="tab-pane active" id="table-income">
                        <table id="income" class="table table-striped table-hover w-100" role="grid">
                            @*<thead>
                                <tr>
                                    <th>@JSMS.Resources.Language.Id</th>
                                    <th>@JSMS.Resources.Language.IncomeType</th>
                                    <th>@JSMS.Resources.Language.Location</th>
                                    <th>@JSMS.Resources.Language.Profile</th>
                                    <th>@JSMS.Resources.Language.Date</th>
                                    <th>@JSMS.Resources.Language.Profile</th>
                                    <th>@JSMS.Resources.Language.Location</th>
                                    <th>@JSMS.Resources.Language.Total</th>
                                    <th>@JSMS.Resources.Language.Purpose</th>
                                    <th>@JSMS.Resources.Language.Invoice</th>
                                    <th>@JSMS.Resources.Language.Payment</th>
                                    <th>@JSMS.Resources.Language.Attachment</th>
                                    <th>@JSMS.Resources.Language.Description</th>
                                    <th>@JSMS.Resources.Language.Created</th>
                                    <th>@JSMS.Resources.Language.Updated</th>
                                    <th>@JSMS.Resources.Language.Actions</th>
                                </tr>
                            </thead>
                            <tbody></tbody>*@
                        </table>
                    </div>
                    <!-- /.tab-pane -->
                    <div class="tab-pane" id="table-expense">
                        <div class="col-md-12">
                            <table id="expense" class="table table-striped table-hover w-100" role="grid">
                                <thead>
                                    <tr>
                                        <th>@JSMS.Resources.Language.Id</th>
                                        <th>@JSMS.Resources.Language.FullName</th>
                                        <th>@JSMS.Resources.Language.Code</th>
                                        <th>@JSMS.Resources.Language.Gender</th>
                                        <th>@JSMS.Resources.Language.ShiftType</th>
                                        <th>@JSMS.Resources.Language.Profile</th>
                                        <th>@JSMS.Resources.Language.Location</th>
                                        <th>@JSMS.Resources.Language.Type</th>
                                        <th>@JSMS.Resources.Language.Main</th>
                                        <th>@JSMS.Resources.Language.Premier</th>
                                        <th>@JSMS.Resources.Language.JoiningDate</th>
                                        <th>@JSMS.Resources.Language.Description</th>
                                        <th>@JSMS.Resources.Language.Created</th>
                                        <th>@JSMS.Resources.Language.Updated</th>
                                        <th>@JSMS.Resources.Language.Actions</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.tab-pane -->
                    <div class="tab-pane" id="table-transaction">
                        <div class="col-md-12">
                            <table id="transaction" class="table table-striped table-hover w-100" role="grid">
                                <thead>
                                    <tr>
                                        <th>@JSMS.Resources.Language.Id</th>
                                        <th>@JSMS.Resources.Language.FullName</th>
                                        <th>@JSMS.Resources.Language.Code</th>
                                        <th>@JSMS.Resources.Language.Gender</th>
                                        <th>@JSMS.Resources.Language.ShiftType</th>
                                        <th>@JSMS.Resources.Language.Profile</th>
                                        <th>@JSMS.Resources.Language.Location</th>
                                        <th>@JSMS.Resources.Language.Type</th>
                                        <th>@JSMS.Resources.Language.Main</th>
                                        <th>@JSMS.Resources.Language.Premier</th>
                                        <th>@JSMS.Resources.Language.JoiningDate</th>
                                        <th>@JSMS.Resources.Language.Description</th>
                                        <th>@JSMS.Resources.Language.Created</th>
                                        <th>@JSMS.Resources.Language.Updated</th>
                                        <th>@JSMS.Resources.Language.Actions</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.tab-pane -->
                </div>
            </div>
        </div>
    </div>
</div>



@Html.Partial("_Income")
@Html.Partial("_Expense")
@section scripts {
    @Scripts.Render("~/Scripts/Functions/Transaction.js")
    @Scripts.Render("~/Scripts/Functions/Income.js")
    @Scripts.Render("~/Scripts/Functions/Expense.js")
}
