<div class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1 class="m-0" style="font-family:'Khmer OS Battambang'">@JSMS.Resources.Language.Dashboard</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">@JSMS.Resources.Language.Home</a></li>
                    <li class="breadcrumb-item active">@JSMS.Resources.Language.Dashboard</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</div>

<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-info">
                    <div class="inner">
                        <h3>@ViewBag.Staff</h3>

                        <p style="font-family:'Khmer OS Battambang'">@JSMS.Resources.Language.Staff @JSMS.Resources.Language.Registration</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-bag"></i>
                    </div>
                    <a href="@Url.Action("Index", "Staff")" style="font-family:'Khmer OS Battambang'" class="small-box-footer">@JSMS.Resources.Language.MoreInfo <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-success">
                    <div class="inner">
                        <h3>@ViewBag.Client</h3>

                        <p style="font-family:'Khmer OS Battambang'">@JSMS.Resources.Language.Client @JSMS.Resources.Language.Registration</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-stats-bars"></i>
                    </div>
                    <a href="@Url.Action("Index", "Client")" style="font-family:'Khmer OS Battambang'" class="small-box-footer">@JSMS.Resources.Language.MoreInfo <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-warning">
                    <div class="inner">
                        <h3>@ViewBag.User</h3>

                        <p style="font-family:'Khmer OS Battambang'">@JSMS.Resources.Language.User @JSMS.Resources.Language.Registration</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-person-add"></i>
                    </div>
                    <a href="@Url.Action("Index", "UserRole")" style="font-family:'Khmer OS Battambang'" class="small-box-footer">@JSMS.Resources.Language.MoreInfo <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-6">
                <!-- small box -->
                <div class="small-box bg-danger">
                    <div class="inner">
                        <h3>65</h3>

                        <p style="font-family:'Khmer OS Battambang'">@JSMS.Resources.Language.OnlineClient @JSMS.Resources.Language.Registration</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-pie-graph"></i>
                    </div>
                    <a href="@Url.Action("Index", "RequestOnline")" style="font-family:'Khmer OS Battambang'" class="small-box-footer">@JSMS.Resources.Language.MoreInfo <i class="fas fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
        </div>
    </div>
</div>

