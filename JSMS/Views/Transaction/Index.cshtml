                </div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("_Income")
@Html.Partial("_IncomeSalary")
@Html.Partial("_Expense")
@Html.Partial("_ExpenseSalary")

@section scripts {
    @Scripts.Render("~/Scripts/Functions/Transaction.js")
    @Scripts.Render("~/Scripts/Functions/Income.js")
    @Scripts.Render("~/Scripts/Functions/IncomeSalary.js")
    @Scripts.Render("~/Scripts/Functions/Expense.js")
    @Scripts.Render("~/Scripts/Functions/ExpenseSalary.js")
}
