<div class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1 class="m-0 text-uppercase" style="font-family:'Khmer OS Battambang'">@JSMS.Resources.Language.RegisterOnlineManagement</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="@Url.Action("Index","Home")">@JSMS.Resources.Language.Home</a>
                    </li>
                    <li class="breadcrumb-item">@JSMS.Resources.Language.RegisterOnline</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</div>
<div id="loading-gif"></div>
<div class="row">
    <div class="col-12">
        <!-- Custom Tabs -->
        <div class="card">
            <div class="card-header  pl-4">
                <div class="row">
                    <div class="col-md-6">
                        <ul class="nav nav-pills ml-auto p-2">
                            <li class="nav-item">
                                <a class="nav-link  active" id="tab-client" href="#table-guarantor" data-toggle="tab">
                                    <i class="fas fa-users"></i>
                                    <span>@JSMS.Resources.Language.OnlineClient</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" id="tab-applicant" href="#table-applicant" data-toggle="tab">
                                    <i class="fas fa-user-friends"></i>
                                    <span>@JSMS.Resources.Language.OnlineApplicant</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6 mt-2">
                        <div class="float-sm-right mr-5">
                            <button type="button" id="show-data-client" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.ShowData</span>
                            </button>
                        </div>
                        <div class="float-sm-right mr-5">
                            <button type="button" id="show-data-applicant" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.ShowData</span>
                            </button>
                        </div>
                    </div>
                </div>

            </div><!-- /.card-header -->
            <div class="card-body">
                <div class="tab-content">
                    <div class="tab-pane active" id="table-guarantor">
                        <table id="client" class="table table-striped table-hover w-100" role="grid">
                            <thead>
                                <tr>
                                    <th>@JSMS.Resources.Language.Id</th>
                                    <th>@JSMS.Resources.Language.FullName</th>
                                    <th>@JSMS.Resources.Language.Gender</th>
                                    <th>@JSMS.Resources.Language.Telephone</th>
                                    <th>@JSMS.Resources.Language.CurrentAddress</th>
                                    <th>@JSMS.Resources.Language.Status</th>
                                    <th>@JSMS.Resources.Language.Description</th>
                                    <th>@JSMS.Resources.Language.Created</th>
                                    <th>@JSMS.Resources.Language.Updated</th>
                                    <th>@JSMS.Resources.Language.Actions</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                    <div class="tab-pane" id="table-applicant">
                        <table id="applicant" class="table table-striped table-hover w-100" role="grid">
                            <thead>
                                <tr>
                                    <th>@JSMS.Resources.Language.Id</th>
                                    <th>@JSMS.Resources.Language.FullName</th>
                                    <th>@JSMS.Resources.Language.Gender</th>
                                    <th>@JSMS.Resources.Language.Education</th>
                                    <th>@JSMS.Resources.Language.Position</th>
                                    <th>@JSMS.Resources.Language.DOB</th>
                                    <th>@JSMS.Resources.Language.Nationality</th>
                                    <th>@JSMS.Resources.Language.Telephone</th>
                                    <th>@JSMS.Resources.Language.CurrentAddress</th>
                                    <th>@JSMS.Resources.Language.Attachment</th>
                                    <th>@JSMS.Resources.Language.Status</th>
                                    <th>@JSMS.Resources.Language.Description</th>
                                    <th>@JSMS.Resources.Language.Created</th>
                                    <th>@JSMS.Resources.Language.Updated</th>
                                    <th>@JSMS.Resources.Language.Actions</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <!-- /.tab-content -->
            </div><!-- /.card-body -->
        </div>
        <!-- ./card -->
    </div>
    <!-- /.col -->
</div>

@*@Html.Partial("_Applicant")
    @Html.Partial("_Behavior")
    @Html.Partial("_Gaurantor")*@

@section scripts {
    @Scripts.Render("~/Scripts/Functions/Online-Applicant.js")
    @Scripts.Render("~/Scripts/Functions/Online-Client.js")
}
