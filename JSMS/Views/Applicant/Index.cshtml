<div class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1 class="m-0 text-uppercase" id="title-applicant">@JSMS.Resources.Language.ApplicantManagement</h1>
                <h1 class="m-0 text-uppercase" id="title-guaranotor">@JSMS.Resources.Language.GuarantorManagement</h1>
                <h3 class="m-0 text-uppercase" id="title-behavior">@JSMS.Resources.Language.BehaviorManagement</h3>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="@Url.Action("Index","Home")">@JSMS.Resources.Language.Home</a>
                    </li>
                    <li class="breadcrumb-item">@JSMS.Resources.Language.Applicant</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</div>
<div id="loading-gif"></div>
<div class="row">
    <div class="col-12">
        <!-- Custom Tabs -->
        <div class="card">
            <div class="card-header  pl-4">
                <div class="row">
                    <div class="col-md-6">
                        <ul class="nav nav-pills ml-auto p-2">
                            <li class="nav-item">
                                <a class="nav-link active" href="#table-applicant" id="tab-applicant" data-toggle="tab">
                                    <i class="fas fa-user-friends"></i>
                                    <span>@JSMS.Resources.Language.Applicant</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#table-guarantor" id="tab-guarantor" data-toggle="tab">
                                    <i class="fas fa-users"></i>
                                    <span>@JSMS.Resources.Language.Guarantor</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#table-behavior" id="tab-behavior" data-toggle="tab">
                                    <i class="fas fa-people-arrows"></i>
                                    <span>@JSMS.Resources.Language.Behavior</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6 mt-2">
                        <div class="float-sm-right mr-5">
                            <button type="button" id="add-new" class="btn btn-primary btn-sm">
                                <i class="fas fa-plus-circle"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.Add</span>
                            </button>
                            <button type="button" id="add-guanrantor" class="btn btn-primary btn-sm">
                                <i class="fas fa-plus-circle"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.Add</span>
                            </button>
                            <button type="button" id="add-bevior" class="btn btn-primary btn-sm">
                                <i class="fas fa-plus-circle"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.Add</span>
                            </button>
                            <button type="button" id="show-data-applicant" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.ShowData</span>
                            </button>
                            <button type="button" id="show-data-guarantor" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.ShowData</span>
                            </button>
                            <button type="button" id="show-data-behavior" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.ShowData</span>
                            </button>
                        </div>
                    </div>
                </div>

            </div><!-- /.card-header -->
            <div class="card-body">
                <div class="tab-content">
                    <div class="tab-pane active" id="table-applicant">
                        <table id="applicant" class="table table-striped table-hover w-100" role="grid">
                            <thead>
                                <tr>
                                    <th>@JSMS.Resources.Language.Id</th>
                                    <th>@JSMS.Resources.Language.FullName</th>
                                    <th>@JSMS.Resources.Language.Gender</th>
                                    <th>@JSMS.Resources.Language.Profile</th>
                                    <th>@JSMS.Resources.Language.Education</th>
                                    <th>@JSMS.Resources.Language.DOB</th>
                                    <th>@JSMS.Resources.Language.Nationality</th>
                                    <th>@JSMS.Resources.Language.Telephone</th>
                                    <th>@JSMS.Resources.Language.CurrentAddress</th>
                                    <th>@JSMS.Resources.Language.Description</th>
                                    <th>@JSMS.Resources.Language.Created</th>
                                    <th>@JSMS.Resources.Language.Updated</th>
                                    <th>@JSMS.Resources.Language.Actions</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                    <!-- /.tab-pane -->
                    <div class="tab-pane" id="table-guarantor">
                        <div class="col-md-12">
                            <table id="guarantor" class="table table-striped table-hover w-100" role="grid">
                                <thead>
                                    <tr>
                                        <th>@JSMS.Resources.Language.Id</th>
                                        <th>@JSMS.Resources.Language.FullName</th>
                                        <th>@JSMS.Resources.Language.Gender</th>
                                        <th>@JSMS.Resources.Language.Profile</th>
                                        <th>@JSMS.Resources.Language.Education</th>
                                        <th>@JSMS.Resources.Language.DOB</th>
                                        <th>@JSMS.Resources.Language.Nationality</th>
                                        <th>@JSMS.Resources.Language.Telephone</th>
                                        <th>@JSMS.Resources.Language.CurrentAddress</th>
                                        <th>@JSMS.Resources.Language.Description</th>
                                        <th>@JSMS.Resources.Language.Created</th>
                                        <th>@JSMS.Resources.Language.Updated</th>
                                        <th>@JSMS.Resources.Language.Actions</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.tab-pane -->
                    <div class="tab-pane" id="table-behavior">
                        <div class="col-md-12">
                            <table id="behavior" class="table table-striped table-hover w-100" role="grid">
                                <thead>
                                    <tr>
                                        <th>@JSMS.Resources.Language.Id</th>
                                        <th>@JSMS.Resources.Language.FullName</th>
                                        <th>@JSMS.Resources.Language.Gender</th>
                                        <th>@JSMS.Resources.Language.Profile</th>
                                        <th>@JSMS.Resources.Language.Telephone</th>
                                        @*<th>@JSMS.Resources.Language.CurrentAddress</th>*@
                                        <th>@JSMS.Resources.Language.Confirmed</th>
                                        <th>@JSMS.Resources.Language.Date</th>
                                        <th>@JSMS.Resources.Language.Attachment</th>
                                        <th>@JSMS.Resources.Language.Description</th>
                                        <th>@JSMS.Resources.Language.Created</th>
                                        <th>@JSMS.Resources.Language.Updated</th>
                                        <th>@JSMS.Resources.Language.Actions</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.tab-pane -->
                </div>
                <!-- /.tab-content -->
            </div><!-- /.card-body -->
        </div>
        <!-- ./card -->
    </div>
    <!-- /.col -->
</div>

@Html.Partial("_Applicant")
@Html.Partial("_Behavior")
@Html.Partial("_Gaurantor")

@section scripts {
    @Scripts.Render("~/Scripts/Functions/Applicant.js")
    @Scripts.Render("~/Scripts/Functions/Behavior.js")
    @Scripts.Render("~/Scripts/Functions/Gaurantor.js")
}
