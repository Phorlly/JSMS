<div class="content-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h1 class="m-0 text-uppercase">Register Online Management</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="@Url.Action("Index","Home")">@JSMS.Resources.Language.Home</a>
                    </li>
                    <li class="breadcrumb-item">@JSMS.Resources.Language.RegisterOnline</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</div>
<div class="row">
    <div class="col-12">
        <!-- Custom Tabs -->
        <div class="card">
            <div class="card-header  pl-4">
                <div class="row">
                    <div class="col-md-6">
                        <ul class="nav nav-pills ml-auto p-2">
                            <li class="nav-item">
                                <a class="nav-link  active" href="#table-guarantor" data-toggle="tab">
                                    <i class="fas fa-users"></i>
                                    <span>@JSMS.Resources.Language.OnlineClient</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#table-applicant" data-toggle="tab">
                                    <i class="fas fa-user-friends"></i>
                                    <span>@JSMS.Resources.Language.OnlineApplicant</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-6 mt-2">
                        <div class="float-sm-right mr-5">
                            <button type="button" id="refresh" class="btn btn-success btn-sm">
                                <i class="fas fa-sync-alt"></i>
                                <span class="text-uppercase">@JSMS.Resources.Language.Refresh</span>
                            </button>
                        </div>
                    </div>
                </div>

            </div><!-- /.card-header -->
            <div class="card-body">
                <div class="tab-content">
                    <div class="tab-pane active" id="table-guarantor">
                        <table id="client" class="table table-striped table-hover w-100" role="grid"></table>
                    </div>
                    <div class="tab-pane" id="table-applicant">
                        <table id="applicant" class="table table-striped table-hover w-100" role="grid"></table>
                    </div>
                </div>
                <!-- /.tab-content -->
            </div><!-- /.card-body -->
        </div>
        <!-- ./card -->
    </div>
    <!-- /.col -->
</div>

@*@Html.Partial("_Applicant")
    @Html.Partial("_Behavior")
    @Html.Partial("_Gaurantor")*@

@section scripts {
    @Scripts.Render("~/Scripts/Functions/Online-Applicant.js")
    @Scripts.Render("~/Scripts/Functions/Online-Client.js")
}
